{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "tft",
	"patterns": [
		{
			"match": "([0-9]{4,4}),([A-Z].*?),([A-Z]),([0-9]{1,4}),([A-Z]|[0-9]),(.*$)",
			"name": "itemdef.tft",
			"captures": {
				"1": {
					"name": "keyword.itemdef.tft"
				},
				"2": {
					"name": "string.itemdef.tft"
				},
				"4": {
					"name": "keyword.itemdef.tft"
				},
				"6": {
					"name": "comment.itemdef.tft"
				}
			}
		}
	],
	"scopeName": "source.tft"
}